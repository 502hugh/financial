{"ast":null,"code":"export default {\n  name: \"ResourcesApply\",\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      resourcesName: null,\n      num: null,\n      fromVisible: false,\n      form: {},\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      ids: [],\n      status: null,\n      employee: \"\"\n    };\n  },\n  mounted() {\n    this.loadEmployee();\n  },\n  created() {\n    this.load(1);\n  },\n  methods: {\n    loadEmployee() {\n      //获取当前员工用户的信息\n      this.$request.get('/employee/selectPowerName').then(res => {\n        if (res.code === '200') {\n          this.employee = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleAdd() {\n      // 新增数据\n      this.form = {}; // 新增数据的时候清空数据\n      this.fromVisible = true; // 打开弹窗\n    },\n\n    handleEdit(row) {\n      // 编辑数据\n      this.form = JSON.parse(JSON.stringify(row)); // 给form对象赋值  注意要深拷贝数据\n      this.fromVisible = true; // 打开弹窗\n    },\n\n    save() {\n      this.form.status = this.status;\n      this.$request.put('/resourceapply/update', this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success('操作成功');\n          this.load(1);\n          this.fromVisible = false;\n          this.status = null;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    del(id) {\n      // 单个删除\n      this.$confirm('您确定删除吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/resourceapply/delete/' + id).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    handleSelectionChange(rows) {\n      // 当前选中的所有的行数据\n      this.ids = rows.map(v => v.id); //  [1,2]\n    },\n\n    delBatch() {\n      // 批量删除\n      if (!this.ids.length) {\n        this.$message.warning('请选择数据');\n        return;\n      }\n      this.$confirm('您确定批量删除这些数据吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/resourceapply/delete/batch', {\n          data: this.ids\n        }).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    load(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/resourceapply/selectPage2', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          resourcesName: this.resourcesName\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    reset() {\n      this.resourcesName = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","resourcesName","num","fromVisible","form","user","JSON","parse","localStorage","getItem","ids","status","employee","mounted","loadEmployee","created","load","methods","$request","get","then","res","code","$message","error","msg","handleAdd","handleEdit","row","stringify","save","put","success","del","id","$confirm","type","response","delete","catch","handleSelectionChange","rows","map","v","delBatch","length","warning","params","list","reset","handleCurrentChange"],"sources":["src/views/manager/ResourcesApply.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"search\">\r\n      <el-input placeholder=\"请输入资产名称\" style=\"width: 200px\" v-model=\"resourcesName\"></el-input>\r\n      <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">查询</el-button>\r\n      <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" stripe  @selection-change=\"handleSelectionChange\">\r\n        <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n<!--        <el-table-column prop=\"id\" label=\"序号\" width=\"80\" align=\"center\" sortable></el-table-column>-->\r\n        <el-table-column prop=\"employeeName\" label=\"员工姓名\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"departmentName\" label=\"所属部门\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"resourcesName\" label=\"资产名称\"></el-table-column>\r\n        <el-table-column prop=\"num\" label=\"申请数量\"></el-table-column>\r\n        <el-table-column prop=\"time\" label=\"申请时间\"></el-table-column>\r\n        <el-table-column prop=\"process\" label=\"当前进度\"></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"审批状态\"></el-table-column>\r\n        <el-table-column prop=\"note\" label=\"审批说明\"></el-table-column>\r\n        <el-table-column v-if=\"user.role === 'ADMIN' || user.level === 2 || employee.powerName === '财务'\" label=\"操作\">\r\n          <template v-slot=\"scope\">\r\n            <el-button type=\"primary\" plain @click=\"handleEdit(scope.row)\">审批</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n\r\n    <el-dialog title=\"审批\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item prop=\"name\" label=\"请审批\">\r\n          <el-select v-model=\"status\" placeholder=\"请选择状态\" style=\"width: 100%\">\r\n            <el-option label=\"审批通过\" value=\"审批通过\"></el-option>\r\n            <el-option label=\"审批不通过\" value=\"审批不通过\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item prop=\"note\" label=\"审批说明\">\r\n          <el-input v-model=\"form.note\" autocomplete=\"off\" placeholder=\"请输入审批说明\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ResourcesApply\",\r\n  data() {\r\n    return {\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      resourcesName: null,\r\n      num: null,\r\n      fromVisible: false,\r\n      form: {},\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      ids: [],\r\n      status: null,\r\n      employee:\"\",\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadEmployee()\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  methods: {\r\n\r\n    loadEmployee(){\r\n      //获取当前员工用户的信息\r\n      this.$request.get('/employee/selectPowerName').then(res => {\r\n        if (res.code === '200') {\r\n          this.employee = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    } ,\r\n    handleAdd() {   // 新增数据\r\n      this.form = {}  // 新增数据的时候清空数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    handleEdit(row) {   // 编辑数据\r\n      this.form = JSON.parse(JSON.stringify(row))  // 给form对象赋值  注意要深拷贝数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    save() {\r\n      this.form.status = this.status\r\n      this.$request.put('/resourceapply/update', this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success('操作成功')\r\n          this.load(1)\r\n          this.fromVisible = false\r\n          this.status = null\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    del(id) {   // 单个删除\r\n      this.$confirm('您确定删除吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/resourceapply/delete/' + id).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    handleSelectionChange(rows) {   // 当前选中的所有的行数据\r\n      this.ids = rows.map(v => v.id)   //  [1,2]\r\n    },\r\n    delBatch() {   // 批量删除\r\n      if (!this.ids.length) {\r\n        this.$message.warning('请选择数据')\r\n        return\r\n      }\r\n      this.$confirm('您确定批量删除这些数据吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/resourceapply/delete/batch', {data: this.ids}).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/resourceapply/selectPage2', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          resourcesName: this.resourcesName,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    reset() {\r\n      this.resourcesName = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAgEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,aAAA;MACAC,GAAA;MACAC,WAAA;MACAC,IAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,GAAA;MACAC,MAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,YAAA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IAEAH,aAAA;MACA;MACA,KAAAI,QAAA,CAAAC,GAAA,8BAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAV,QAAA,GAAAS,GAAA,CAAAzB,IAAA;QACA;UACA,KAAA2B,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAC,UAAA;MAAA;MACA,KAAAtB,IAAA;MACA,KAAAD,WAAA;IACA;;IACAwB,WAAAC,GAAA;MAAA;MACA,KAAAxB,IAAA,GAAAE,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAuB,SAAA,CAAAD,GAAA;MACA,KAAAzB,WAAA;IACA;;IACA2B,KAAA;MACA,KAAA1B,IAAA,CAAAO,MAAA,QAAAA,MAAA;MACA,KAAAO,QAAA,CAAAa,GAAA,+BAAA3B,IAAA,EAAAgB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAS,OAAA;UACA,KAAAhB,IAAA;UACA,KAAAb,WAAA;UACA,KAAAQ,MAAA;QACA;UACA,KAAAY,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAQ,IAAAC,EAAA;MAAA;MACA,KAAAC,QAAA;QAAAC,IAAA;MAAA,GAAAhB,IAAA,CAAAiB,QAAA;QACA,KAAAnB,QAAA,CAAAoB,MAAA,4BAAAJ,EAAA,EAAAd,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YAAA;YACA,KAAAC,QAAA,CAAAS,OAAA;YACA,KAAAhB,IAAA;UACA;YACA,KAAAO,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QACA;MACA,GAAAc,KAAA,QACA;IACA;IACAC,sBAAAC,IAAA;MAAA;MACA,KAAA/B,GAAA,GAAA+B,IAAA,CAAAC,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAT,EAAA;IACA;;IACAU,SAAA;MAAA;MACA,UAAAlC,GAAA,CAAAmC,MAAA;QACA,KAAAtB,QAAA,CAAAuB,OAAA;QACA;MACA;MACA,KAAAX,QAAA;QAAAC,IAAA;MAAA,