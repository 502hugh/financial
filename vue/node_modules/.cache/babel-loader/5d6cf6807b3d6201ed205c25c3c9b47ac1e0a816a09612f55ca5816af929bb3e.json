{"ast":null,"code":"export default {\n  name: \"Reimbursement\",\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      fromVisible: false,\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      form: {},\n      rules: {},\n      ids: []\n    };\n  },\n  created() {\n    this.load(1);\n  },\n  mounted() {},\n  methods: {\n    save() {\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request({\n            url: this.form.id ? '/reimbursement/update' : '/reimbursement/add',\n            method: this.form.id ? 'PUT' : 'POST',\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              // 表示成功保存\n              this.$message.success('操作成功');\n              this.load(1);\n              this.fromVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n\n    del(id) {\n      this.$confirm('您确定删除吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete(\"/reimbursement/delete\" + id).then(res => {\n          if (res.code === \"200\") {\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      });\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row)); // 给form对象赋值  注意要深拷贝数据\n      this.fromVisible = true; // 打开弹窗\n    },\n\n    handleSelectionChange(rows) {\n      // 当前选中的所有的行数据\n      this.ids = rows.map(v => v.id); //  [1,2]\n    },\n\n    delBatch() {\n      if (!this.ids.length) {\n        this.$message.warning('请选择数据');\n        return;\n      }\n      this.$confirm('您确定批量删除这些数据吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete(\"/reimbursement/delete/batch\", {\n          data: this.ids\n        }).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      });\n    },\n\n    handleAdd() {\n      this.form = {}; // 新增数据的时候清空数据\n      this.fromVisible = true; // 打开弹窗\n    },\n\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get(\"/reimbursement/selectPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.table = res.data?.total;\n      });\n    },\n    reset() {\n      this.name = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    handlePictureSuccess(response, file, fileList) {\n      this.form.img = response.data;\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","fromVisible","user","JSON","parse","localStorage","getItem","form","rules","ids","created","load","mounted","methods","save","$refs","formRef","validate","valid","$request","url","id","method","then","res","code","$message","success","error","msg","del","$confirm","type","response","delete","handleEdit","row","stringify","handleSelectionChange","rows","map","v","delBatch","length","warning","handleAdd","get","params","list","table","reset","handleCurrentChange","handlePictureSuccess","file","fileList","img"],"sources":["src/views/manager/voucher/Reimbursement.vue"],"sourcesContent":["\r\n\r\n<template>\r\n\r\n  <div>\r\n    <div class=\"search\">\r\n      <el-input placeholder=\"请输入申请人名称\" style=\"width: 200px\" v-model=\"name\"></el-input>\r\n      <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">查询</el-button>\r\n      <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div class=\"operation\">\r\n      <el-button type=\"primary\" plain @click=\"handleAdd(user)\">新增</el-button>\r\n      <el-button type=\"danger\" plain @click=\"delBatch\">批量删除</el-button>\r\n    </div>\r\n\r\n\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" stripe  @selection-change=\"handleSelectionChange\">\r\n        <el-table-column v-if=\"user.role == 'ADMIN'\" type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"报账人\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"time\" label=\"报账时间\"></el-table-column>\r\n        <el-table-column prop=\"type\" label=\"报账类型\"></el-table-column>\r\n        <el-table-column prop=\"note\" label=\"备注\"></el-table-column>\r\n<!--        <el-table-column prop=\"img\" label=\"资产图片\" show-overflow-tooltip>-->\r\n<!--          <template v-slot=\"scope\">-->\r\n<!--            <div style=\"display: flex; align-items: center\">-->\r\n<!--              <el-image style=\"width: 40px; height: 40px; border-radius: 50%\" v-if=\"scope.row.img\"-->\r\n<!--                        :src=\"scope.row.img\" :preview-src-list=\"[scope.row.img]\"></el-image>-->\r\n<!--            </div>-->\r\n<!--          </template>-->\r\n<!--        </el-table-column>-->\r\n\r\n\r\n        <el-table-column label=\"操作\" width=\"180\" align=\"center\">\r\n          <template v-slot=\"scope\" v-if=\"user.role === 'ADMIN'\">\r\n            <el-button plain type=\"primary\" @click=\"handleEdit(scope.row)\" size=\"mini\">编辑</el-button>\r\n            <el-button plain type=\"danger\" size=\"mini\" @click=del(scope.row.id)>删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n\r\n    </div>\r\n\r\n    <el-dialog title=\"申请\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n        <el-form :model=\"form\" label-width=\"100px\" style=\"padding-right: 50px\" :rules=\"rules\" ref=\"formRef\">\r\n          <el-form-item label=\"类型\" prop=\"type\">\r\n            <el-input v-model=\"form.type\" placeholder=\"类型\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"报账人\" :prop=\"user.name\">\r\n            <el-input  v-model=\"form.name\" :placeholder=\"user.name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"备注\" prop=\"note\">\r\n            <el-input v-model=\"form.note\" placeholder=\"备注\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"凭证\">\r\n            <el-upload\r\n                class=\"avatar-uploader\"\r\n                :action=\"$baseUrl + '/reimbursement/upload'\"\r\n                :headers=\"{ token: user.token }\"\r\n                list-type=\"picture\"\r\n                :on-success=\"handlePictureSuccess\"\r\n            >\r\n              <el-button type=\"primary\">上传凭证</el-button>\r\n            </el-upload>\r\n          </el-form-item>\r\n        </el-form>\r\n\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n        </div>\r\n    </el-dialog>\r\n\r\n\r\n  </div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Reimbursement\",\r\n  data(){\r\n    return{\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      fromVisible: false,\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      form: {},\r\n      rules: {\r\n\r\n      },\r\n      ids: []\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  mounted() {\r\n  },\r\n  methods:{\r\n    save(){\r\n      this.$refs.formRef.validate((valid) =>{\r\n        if(valid){\r\n          this.$request({\r\n            url: this.form.id ? '/reimbursement/update' : '/reimbursement/add',\r\n            method: this.form.id ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {  // 表示成功保存\r\n              this.$message.success('操作成功')\r\n              this.load(1)\r\n              this.fromVisible = false\r\n            } else {\r\n              this.$message.error(res.msg)  // 弹出错误的信息\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    del(id){\r\n      this.$confirm('您确定删除吗？', '确认删除', {type: \"warning\"}).then(response =>{\r\n        this.$request.delete(\"/reimbursement/delete\"+id).then(res=>{\r\n          if(res.code === \"200\"){\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      })\r\n    },\r\n\r\n    handleEdit(row){\r\n      this.form = JSON.parse(JSON.stringify(row))  // 给form对象赋值  注意要深拷贝数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    handleSelectionChange(rows) {   // 当前选中的所有的行数据\r\n      this.ids = rows.map(v => v.id)   //  [1,2]\r\n    },\r\n    delBatch(){\r\n      if (!this.ids.length) {\r\n        this.$message.warning('请选择数据')\r\n        return\r\n      }\r\n      this.$confirm('您确定批量删除这些数据吗？', '确认删除',{type: \"warning\"}).then(response => {\r\n        this.$request.delete(\"/reimbursement/delete/batch\",{data: this.ids}).then(res =>{\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      })\r\n    },\r\n\r\n    handleAdd(){\r\n      this.form = {}  // 新增数据的时候清空数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    load(pageNum){\r\n      if(pageNum) this.pageNum = pageNum\r\n      this.$request.get(\"/reimbursement/selectPage\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n          name:this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.table = res.data?.total\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n    handlePictureSuccess(response, file, fileList) {\r\n      this.form.img = response.data\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n\r\n<!-- 报账-->"],"mappings":"AA8FA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,IAAA;MACAC,KAAA,GAEA;MACAC,GAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,QAAA,GACA;EACAC,OAAA;IACAC,KAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAC,QAAA;YACAC,GAAA,OAAAb,IAAA,CAAAc,EAAA;YACAC,MAAA,OAAAf,IAAA,CAAAc,EAAA;YACAzB,IAAA,OAAAW;UACA,GAAAgB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cAAA;cACA,KAAAC,QAAA,CAAAC,OAAA;cACA,KAAAhB,IAAA;cACA,KAAAV,WAAA;YACA;cACA,KAAAyB,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;YACA;UACA;QACA;MACA;IACA;;IACAC,IAAAT,EAAA;MACA,KAAAU,QAAA;QAAAC,IAAA;MAAA,GAAAT,IAAA,CAAAU,QAAA;QACA,KAAAd,QAAA,CAAAe,MAAA,2BAAAb,EAAA,EAAAE,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAAC,QAAA,CAAAC,OAAA;YACA,KAAAhB,IAAA;UACA;YACA,KAAAe,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;UACA;QACA;MACA;IACA;;IAEAM,WAAAC,GAAA;MACA,KAAA7B,IA