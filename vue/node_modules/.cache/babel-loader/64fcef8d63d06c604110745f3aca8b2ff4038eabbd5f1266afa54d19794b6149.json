{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport employee from \"@/views/manager/user/Employee.vue\";\nexport default {\n  name: \"Manager\",\n  data() {\n    return {\n      employee: \"\",\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}')\n    };\n  },\n  mounted() {\n    this.loadEmployee();\n  },\n  created() {\n    if (!this.user.id) {\n      this.$router.push('/login');\n    }\n  },\n  methods: {\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}'); // 重新获取下用户的最新信息\n    },\n\n    goToPerson() {\n      if (this.user.role === 'ADMIN') {\n        this.$router.push('/adminPerson');\n      }\n      if (this.user.role === 'USER') {\n        this.$router.push('/employeePerson');\n      }\n    },\n    logout() {\n      localStorage.removeItem('xm-user');\n      this.$router.push('/login');\n    },\n    loadEmployee() {\n      //获取当前员工用户的信息\n      this.$request.get('/employee/selectPowerName').then(res => {\n        if (res.code === '200') {\n          this.employee = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["employee","name","data","user","JSON","parse","localStorage","getItem","mounted","loadEmployee","created","id","$router","push","methods","updateUser","goToPerson","role","logout","removeItem","$request","get","then","res","code","$message","error","msg"],"sources":["src/views/Manager.vue"],"sourcesContent":["<template>\r\n  <div class=\"manager-container\">\r\n    <!--  头部  -->\r\n    <div class=\"manager-header\">\r\n      <div class=\"manager-header-left\">\r\n        <img src=\"@/assets/imgs/caiwu.svg\" />\r\n        <div class=\"title\">财务管理</div>\r\n      </div>\r\n\r\n      <div class=\"manager-header-center\">\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n          <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n          <el-breadcrumb-item :to=\"{ path: $route.path }\">{{ $route.meta.name }}</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n      </div>\r\n\r\n      <div class=\"manager-header-right\">\r\n        <el-dropdown placement=\"bottom\">\r\n          <div class=\"avatar\">\r\n            <img :src=\"user.avatar || 'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\" />\r\n            <div>{{ user.name ||  \"用户\" }}</div>\r\n          </div>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item @click.native=\"goToPerson\">个人信息</el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"$router.push('/password')\">修改密码</el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"logout\">退出登录</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n    </div>\r\n\r\n    <!--  主体  -->\r\n    <div class=\"manager-main\">\r\n      <!--  侧边栏  -->\r\n      <div v-if=\"user.role === 'ADMIN'\" class=\"manager-main-left\">\r\n        <el-menu :default-openeds=\"['info', 'user','matters','zongzhang']\" router style=\"border: none\" :default-active=\"$route.path\">\r\n          <el-menu-item index=\"/home\">\r\n            <i class=\"el-icon-s-home\"></i>\r\n            <span slot=\"title\">系统首页</span>\r\n          </el-menu-item>\r\n          <el-submenu index=\"info\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>信息管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/notice\">公告信息</el-menu-item>\r\n            <el-menu-item index=\"/department\">部门信息</el-menu-item>\r\n          </el-submenu>\r\n\r\n          <el-submenu index=\"user\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>用户管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/admin\">管理员信息</el-menu-item>\r\n            <el-menu-item index=\"/employee\">员工信息</el-menu-item>\r\n          </el-submenu>\r\n          <el-submenu index=\"matters\">\r\n            <template slot=\"title\">\r\n              <img src=\"@/assets/imgs/caiwu.svg\" />\r\n              <span>资产管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/resources\">资产信息</el-menu-item>\r\n            <el-menu-item index=\"/resourcesApply\">资产审批</el-menu-item>\r\n            <el-menu-item index=\"/resourcesMy\">员工资产</el-menu-item>\r\n          </el-submenu>\r\n          <el-submenu index=\"zongzhang\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>总账</span>\r\n            </template>\r\n            <el-menu-item index=\"/salary\">薪资信息</el-menu-item>\r\n            <el-menu-item index=\"/salaryManager\">薪资管理</el-menu-item>\r\n            <el-menu-item index=\"/financialout\">财务支出</el-menu-item>\r\n            <el-menu-item index=\"/financialin\">财务收入</el-menu-item>\r\n            <el-menu-item index=\"/reimbursement\">报账</el-menu-item>\r\n            <el-menu-item index=\"/reimbursementApply\">报账审批</el-menu-item>\r\n\r\n\r\n          </el-submenu>\r\n        </el-menu>\r\n      </div>\r\n\r\n\r\n      <div v-if=\"user.role === 'USER'\" class=\"manager-main-left\">\r\n        <el-menu :default-openeds=\"['user','matters', 'zongzhang']\" router style=\"border: none\" :default-active=\"$route.path\">\r\n          <el-menu-item index=\"/home\">\r\n            <i class=\"el-icon-s-home\"></i>\r\n            <span slot=\"title\">系统首页</span>\r\n          </el-menu-item>\r\n          <el-submenu index=\"user\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>信息管理</span>\r\n            </template>\r\n            <el-menu-item  v-if=\"employee.powerName === '运营'\" index=\"/notice\">公告信息</el-menu-item>\r\n            <el-menu-item index=\"/employee\">员工信息</el-menu-item>\r\n          </el-submenu>\r\n\r\n          <el-submenu index=\"matters\">\r\n            <template slot=\"title\">\r\n              <img src=\"@/assets/imgs/caiwu.svg\" /><span>资产信息</span>\r\n            </template>\r\n            <el-menu-item index=\"/resources\">资产信息</el-menu-item>\r\n            <el-menu-item index=\"/resourcesMy\">员工资产</el-menu-item>\r\n            <el-menu-item v-if=\"user.level === 2 || employee.powerName === '财务'\" index=\"/resourcesApply\">资产审批</el-menu-item>\r\n            <el-submenu index=\"zongzhang\">\r\n              <template slot=\"title\">\r\n                <i class=\"el-icon-menu\"></i><span>总账</span>\r\n              </template>\r\n              <el-menu-item index=\"/salary\">薪资信息</el-menu-item>\r\n              <el-menu-item v-if=\"employee.powerName === '财务'\" index=\"/salaryManager\">薪资管理</el-menu-item>\r\n              <el-menu-item v-if=\"user.level === 2\" index=\"/financialout\">财务支出</el-menu-item>\r\n              <el-menu-item v-if=\"user.level === 2\" index=\"/financialin\">财务收入</el-menu-item>\r\n              <el-menu-item  index=\"/reimbursement\">报账</el-menu-item>\r\n              <el-menu-item v-if=\"employee.powerName === '财务'\" index=\"/reimbursementApply\">报账审批</el-menu-item>\r\n\r\n            </el-submenu>\r\n          </el-submenu>\r\n\r\n\r\n        </el-menu>\r\n      </div>\r\n      <!--  数据表格  -->\r\n      <div class=\"manager-main-right\">\r\n        <router-view @update:user=\"updateUser\" />\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport employee from \"@/views/manager/user/Employee.vue\";\r\n\r\nexport default {\r\n  name: \"Manager\",\r\n  data() {\r\n    return {\r\n      employee: \"\",\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadEmployee()\r\n  },\r\n  created() {\r\n    if (!this.user.id) {\r\n      this.$router.push('/login')\r\n    }\r\n  },\r\n  methods: {\r\n\r\n    updateUser() {\r\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}')   // 重新获取下用户的最新信息\r\n    },\r\n    goToPerson() {\r\n      if (this.user.role === 'ADMIN') {\r\n        this.$router.push('/adminPerson')\r\n      }\r\n      if (this.user.role === 'USER') {\r\n        this.$router.push('/employeePerson')\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem('xm-user')\r\n      this.$router.push('/login')\r\n    },\r\n    loadEmployee(){\r\n      //获取当前员工用户的信息\r\n      this.$request.get('/employee/selectPowerName').then(res => {\r\n        if (res.code === '200') {\r\n          this.employee = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    } ,\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import \"@/assets/css/manager.css\";\r\n</style>"],"mappings":";AAiIA,OAAAA,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAF,QAAA;MACAG,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,YAAA;EACA;EACAC,QAAA;IACA,UAAAP,IAAA,CAAAQ,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;EACAC,OAAA;IAEAC,WAAA;MACA,KAAAZ,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;;IACAS,WAAA;MACA,SAAAb,IAAA,CAAAc,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA;MACA,SAAAV,IAAA,CAAAc,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA;IACA;IACAK,OAAA;MACAZ,YAAA,CAAAa,UAAA;MACA,KAAAP,OAAA,CAAAC,IAAA;IACA;IACAJ,aAAA;MACA;MACA,KAAAW,QAAA,CAAAC,GAAA,8BAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAxB,QAAA,GAAAuB,GAAA,CAAArB,IAAA;QACA;UACA,KAAAuB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;EAEA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}