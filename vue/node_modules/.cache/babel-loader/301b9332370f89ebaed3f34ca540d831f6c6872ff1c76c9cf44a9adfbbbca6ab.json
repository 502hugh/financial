{"ast":null,"code":"export default {\n  name: \"Reimbursement\",\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      name: '',\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      form: {},\n      rules: {},\n      ids: [],\n      rowid: 0,\n      pictureData: [],\n      pictureVisible: false\n    };\n  },\n  created() {\n    this.load(1);\n  },\n  methods: {\n    openPictureDialog(row) {\n      if (row.id) this.id = row.id;\n      this.$request.get(\"/reimbursement/selectPictureByReimbursementId/\" + row.id).then(res => {\n        if (res.code === \"200\") {\n          this.pictureData = res.data?.list;\n          this.rowid = 0;\n        } else {\n          this.$message.error(res.msg); // 弹出错误的信息\n        }\n      });\n\n      this.pictureVisible = true; // 打开弹窗\n      this.pictureDialogUsernameId = row.id; // 把报账id赋值给下个表\n    },\n\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get(\"/reimbursement/selectPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        console.log(this.tableData);\n        this.table = res.data?.total;\n      });\n    },\n    reset() {\n      this.name = null;\n      this.load(1);\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","user","JSON","parse","localStorage","getItem","form","rules","ids","rowid","pictureData","pictureVisible","created","load","methods","openPictureDialog","row","id","$request","get","then","res","code","list","$message","error","msg","pictureDialogUsernameId","params","console","log","table","reset"],"sources":["src/views/manager/voucher/ReimbursementApply.vue"],"sourcesContent":["<template>\r\n\r\n  <div>\r\n    <div class=\"search\">\r\n      <el-input placeholder=\"请输入申请人名称\" style=\"width: 200px\" v-model=\"name\"></el-input>\r\n      <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">查询</el-button>\r\n      <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" stripe>\r\n        <el-table-column prop=\"name\" label=\"报账人\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"time\" label=\"报账时间\"></el-table-column>\r\n        <el-table-column prop=\"amount\" label=\"报账金额\"></el-table-column>\r\n        <el-table-column prop=\"type\" label=\"报账类型\"></el-table-column>\r\n        <el-table-column prop=\"note\" label=\"备注\"></el-table-column>\r\n\r\n        <el-table-column label=\"附件\">\r\n          <template v-slot=\"scope\">\r\n            <el-button type=\"text\" @click=\"openPictureDialog(scope.row)\">\r\n              <i class=\"el-icon-picture-outline\"></i> <!-- 使用一个图片图标 -->\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n\r\n\r\n        <el-table-column label=\"操作\" width=\"250\" align=\"center\">\r\n          <template v-slot=\"scope\">\r\n            <el-button plain type=\"primary\" @click=\"handleApply(scope.row)\" size=\"mini\">审批</el-button>\r\n          </template>\r\n        </el-table-column>\r\n\r\n      </el-table>\r\n\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n\r\n    </div>\r\n\r\n\r\n    <el-dialog title=\"附件详情\" :visible.sync=\"pictureVisible\" width=\"50%\" height=\"50%\":close-on-click-modal=\"false\" destroy-on-close>\r\n      <div class=\"table\">\r\n        <el-table :data=\"pictureData\" stripe>\r\n          <el-table-column width=\"180\" >\r\n            <span>{{rowid+1}}</span>\r\n          </el-table-column>\r\n          <el-table-column label=\"图片\">\r\n            <template v-slot=\"scope\">\r\n              <div style=\"display: flex; align-items: center\">\r\n                <el-image  style=\"width: 140px; height: 140px;\" v-if=\"scope.row.picture\"\r\n                           :src=\"scope.row.picture\" :preview-src-list=\"[scope.row.picture]\"></el-image>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Reimbursement\",\r\n  data(){\r\n    return{\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      name:'',\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      form: {},\r\n      rules: {\r\n      },\r\n      ids: [],\r\n      rowid:0,\r\n      pictureData:[],\r\n      pictureVisible:false,\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  methods:{\r\n\r\n\r\n    openPictureDialog(row){\r\n      if(row.id) this.id = row.id\r\n      this.$request.get(\"/reimbursement/selectPictureByReimbursementId/\" + row.id).then(res =>{\r\n        if(res.code === \"200\"){\r\n          this.pictureData = res.data?.list\r\n          this.rowid = 0\r\n        } else {\r\n          this.$message.error(res.msg)  // 弹出错误的信息\r\n        }\r\n      })\r\n      this.pictureVisible = true   // 打开弹窗\r\n      this.pictureDialogUsernameId = row.id // 把报账id赋值给下个表\r\n    },\r\n    load(pageNum){\r\n      if(pageNum) this.pageNum = pageNum\r\n      this.$request.get(\"/reimbursement/selectPage\",{\r\n        params:{\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n\r\n        console.log(this.tableData)\r\n        this.table = res.data?.total\r\n      })\r\n\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.load(1)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n\r\n<!-- 报账-->"],"mappings":"AA0EA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAL,IAAA;MACAM,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,IAAA;MACAC,KAAA,GACA;MACAC,GAAA;MACAC,KAAA;MACAC,WAAA;MACAC,cAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IAGAC,kBAAAC,GAAA;MACA,IAAAA,GAAA,CAAAC,EAAA,OAAAA,EAAA,GAAAD,GAAA,CAAAC,EAAA;MACA,KAAAC,QAAA,CAAAC,GAAA,oDAAAH,GAAA,CAAAC,EAAA,EAAAG,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAZ,WAAA,GAAAW,GAAA,CAAAzB,IAAA,EAAA2B,IAAA;UACA,KAAAd,KAAA;QACA;UACA,KAAAe,QAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;;MACA,KAAAf,cAAA;MACA,KAAAgB,uBAAA,GAAAX,GAAA,CAAAC,EAAA;IACA;;IACAJ,KAAAf,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAoB,QAAA,CAAAC,GAAA;QACAS,MAAA;UACA9B,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAJ,IAAA,OAAAA;QACA;MACA,GAAAyB,IAAA,CAAAC,GAAA;QACA,KAAAxB,SAAA,GAAAwB,GAAA,CAAAzB,IAAA,EAAA2B,IAAA;QAEAM,OAAA,CAAAC,GAAA,MAAAjC,SAAA;QACA,KAAAkC,KAAA,GAAAV,GAAA,CAAAzB,IAAA,EAAAI,KAAA;MACA;IAEA;IACAgC,MAAA;MACA,KAAArC,IAAA;MACA,KAAAkB,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}