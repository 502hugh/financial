etItem('xm-user') || '{}'),\r\n\r\n\r\n      name:'',\r\n      employee:\"\",\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadEmployee()\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  methods:{\r\n\r\n\r\n    handleEdit(row){\r\n\r\n    },\r\n    del(id){\r\n\r\n    },\r\n\r\n    handleSelectionChange(rows){\r\n      this.ids = rows.map(v => v.id)   //  [1,2]\r\n    },\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/salary/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          year: this.year,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.load(1)\r\n    },\r\n\r\n\r\n    loadEmployeePowername(){\r\n      //获取当前员工用户的信息\r\n      this.$request.get('/employee/selectPowerName').then(res => {\r\n        if (res.code === '200') {\r\n          this.employeePower = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    } ,\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA2CA,OAAAA,QAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MAGAV,IAAA;MACAD,QAAA;IACA;EACA;EACAY,QAAA;IACA,KAAAC,YAAA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IAGAC,WAAAC,GAAA,GAEA;IACAC,IAAAC,EAAA,GAEA;IAEAC,sBAAAC,IAAA;MACA,KAAAC,GAAA,GAAAD,IAAA,CAAAE,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAL,EAAA;IACA;;IACAL,KAAAX,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAsB,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAxB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAwB,IAAA,OAAAA;QACA;MACA,GAAAC,IAAA,CAAAC,GAAA;QACA,KAAA5B,SAAA,GAAA4B,GAAA,CAAA7B,IAAA,EAAA8B,IAAA;QACA,KAAA1B,KAAA,GAAAyB,GAAA,CAAA7B,IAAA,EAAAI,KAAA;MACA;IACA;IACA2B,MAAA;MACA,KAAAhC,IAAA;MACA