{"ast":null,"code":"export default {\n  name: \"Reimbursement\",\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      name: '',\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      form: {},\n      rules: {},\n      ids: [],\n      rowid: 0,\n      pictureData: [],\n      pictureVisible: false,\n      applyVisible: false,\n      status: null\n    };\n  },\n  created() {\n    this.load(1);\n  },\n  methods: {\n    handleApply(id) {\n      if (id) this.id = id;\n      this.$request.get(\"/voucherPicture/selectByReimbursementId/\" + id).then(res => {\n        if (res.code === \"200\") {\n          this.form = res.data?.list;\n          this.rowid = 0;\n        } else {\n          this.$message.error(res.msg); // 弹出错误的信息\n        }\n      });\n\n      this.applyVisible = true;\n    },\n    openPictureDialog(row) {\n      if (row.id) this.id = row.id;\n      this.$request.get(\"/reimbursement/selectPictureByReimbursementId/\" + row.id).then(res => {\n        if (res.code === \"200\") {\n          this.pictureData = res.data?.list;\n          this.rowid = 0;\n        } else {\n          this.$message.error(res.msg); // 弹出错误的信息\n        }\n      });\n\n      this.pictureVisible = true; // 打开弹窗\n      this.pictureDialogUsernameId = row.id; // 把报账id赋值给下个表\n    },\n\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get(\"/reimbursement/selectPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        console.log(this.tableData);\n        this.table = res.data?.total;\n      });\n    },\n    reset() {\n      this.name = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["name