 </el-table>\r\n\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n\r\n    </div>\r\n\r\n\r\n    <el-dialog title=\"附件详情\" :visible.sync=\"pictureVisible\" width=\"50%\" height=\"50%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <div class=\"table\">\r\n        <el-table :data=\"pictureData\" stripe>\r\n          <el-table-column label=\"图片\">\r\n            <template v-slot=\"scope\">\r\n              <div style=\"display: flex; align-items: center\">\r\n                <el-image  style=\"width: 140px; height: 140px;\" v-if=\"scope.row.picture\"\r\n                           :src=\"scope.row.picture\" :preview-src-list=\"[scope.row.picture]\"></el-image>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"审批\" :visible.sync=\"applyVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form :model=\"form\" label-width=\"100px\" style=\"padding-right: 50px\"  ref=\"formRef\">\r\n        <el-form-item prop=\"name\" label=\"请审批\">\r\n          <el-select v-model=\"status\" placeholder=\"请选择状态\" style=\"width: 100%\">\r\n            <el-option label=\"审批通过\" value=\"审批通过\"></el-option>\r\n            <el-option label=\"审批不通过\" value=\"审批不通过\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item prop=\"note\" label=\"审批说明\">\r\n          <el-input v-model=\"form.note\" autocomplete=\"off\" placeholder=\"请输入审批说明\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"applyVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"saveChange\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n\r\n\r\n  </div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Reimbursement\",\r\n  data(){\r\n    return{\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      name:'',\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      form: {},\r\n      rules: {\r\n      },\r\n      ids: [],\r\n      rowid:0,\r\n      pictureData:[],\r\n      pictureVisible:false,\r\n      applyVisible:false,\r\n      status:null,\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  methods:{\r\n\r\n\r\n    handleApply(id){\r\n      if(id) this.id = id\r\n      this.$request.get(\"/voucherPicture/selectByReimbursementId/\" + id).then(res =>{\r\n        if(res.code === \"200\"){\r\n          this.form = res.data?.list\r\n          this.rowid = 0\r\n        } else {\r\n          this.$message.error(res.msg)  // 弹出错误的信息\r\n        }\r\n      })\r\n      this.applyVisible = true\r\n    },\r\n\r\n    openPictureDialog(row){\r\n      if(row.id) this.id = row.id\r\n      this.$request.get(\"/reimbursement/selectPictureByReimbursementId/\" + row.id).then(res =>{\r\n        if(res.code === \"200\"){\r\n          this.pictureData = res.data?.list\r\n          this.rowid = 0\r\n        } else {\r\n          this.$message.error(res.msg)  // 弹出错误的信息\r\n        }\r\n      })\r\n      this.pictureVisible = true   // 打开弹窗\r\n      this.pictureDialogUsernameId = row.id // 把报账id赋值给下个表\r\n    },\r\n    load(pageNum){\r\n      if(pageNum) this.pageNum = pageNum\r\n      this.$request.get(\"/reimbursement/selectPage\",{\r\n        params:{\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        console.log(this.tableData)\r\n        this.table = res.data?.total\r\n      })\r\n\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n\r\n<!-- 报账-->"],"mappings":"AA4FA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAL,IAAA;MACAM,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,IAAA;MACAC,KAAA,GACA;MACAC,GAAA;MACAC,KAAA;MACAC,WAAA;MACAC,cAAA;MACAC,YAAA;MACAC,MAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IAGAC,YAAAC,EAAA;MACA,IAAAA,EAAA,OAAAA,EAAA,GAAAA,EAAA;MACA,KAAAC,QAAA,CAAAC,GAAA,8CAAAF,EAAA,EAAAG,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAjB,IAAA,GAAAgB,GAAA,CAAA1B,IAAA,EAAA4B,IAAA;UACA,KAAAf,KAAA;QACA;UACA,KAAAgB,QAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;;MACA,KAAAf,YAAA;IACA;IAEAgB,kBAAAC,GAAA;MACA,IAAAA,GAAA,CAAAX,EAAA,OAAAA,EAAA,GAAAW,GAAA,CAAAX,EAAA;MACA,KAAAC,QAAA,CAAAC,GAAA,oDAAAS,GAAA,CAAAX,EAAA,EAAAG,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAb,WAAA,GAAAY,GAAA,CAAA1B,IAAA,EAAA4B,IAAA;UACA,KAAAf,KAAA;QACA;UACA,KAAAgB,QAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;;MACA,KAAAhB,cAAA;MACA,KAAAmB,uBAAA,GAAAD,GAAA,CAAAX,EAAA;IACA;;IACAH,KAAAjB,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAqB,QAAA,CAAAC,GAAA;QACAW,MAAA;UACAjC,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAJ,IAAA,OAAAA;QACA;MACA,GAAA0B,IAAA,CAAAC,GAAA;QACA,KAAAzB,SAAA,GAAAyB,GAAA,CAAA1B,IAAA,EAAA4B,IAAA;QACAQ,OAAA,CAAAC,GAAA,MAAApC,SAAA;QACA,KAAAqC,KAAA,GAAAZ,GAAA,CAAA1B,IAAA,EAAAI,KAAA;MACA;IAEA;IACAmC,MAAA;MACA,KAAAxC,IAAA;MACA,KAAAoB,IAAA;IACA;IACAqB,oBAAAtC,OAAA;MACA,KAAAiB,IAAA,CAAAjB,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}