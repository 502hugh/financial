{"ast":null,"code":"export default {\n  name: \"Reimbursement\",\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      fromVisible: false,\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入名称',\n          trigger: 'blur'\n        }]\n      },\n      ids: []\n    };\n  },\n  created() {\n    this.load(1);\n  },\n  mounted() {},\n  methods: {\n    delBatch() {\n      if (!this.ids.length) {\n        this.$message.warning('请选择数据');\n        return;\n      }\n      this.$confirm('您确定批量删除这些数据吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete(\"\");\n      });\n    },\n    handleAdd() {\n      this.form = {}; // 新增数据的时候清空数据\n      this.fromVisible = true; // 打开弹窗\n    },\n\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get(\"/reimbursement/selectPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.table = res.data?.total;\n      });\n    },\n    reset() {\n      this.name = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    handlePictureSuccess(response, file, fileList) {\n      this.form.img = response.data;\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","fromVisible","user","JSON","parse","localStorage","getItem","rules","required","message","trigger","ids","created","load","mounted","methods","delBatch","length","$message","warning","$confirm","type","then","response","$request","delete","handleAdd","form","get","params","res","list","table","reset","handleCurrentChange","handlePictureSuccess","file","fileList","img"],"sources":["src/views/manager/voucher/Reimbursement.vue"],"sourcesContent":["\r\n\r\n<template>\r\n\r\n  <div>\r\n    <div class=\"search\">\r\n      <el-input placeholder=\"请输入申请人名称\" style=\"width: 200px\" v-model=\"name\"></el-input>\r\n      <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">查询</el-button>\r\n      <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div class=\"operation\">\r\n      <el-button type=\"primary\" plain @click=\"handleAdd\">新增</el-button>\r\n      <el-button type=\"danger\" plain @click=\"delBatch\">批量删除</el-button>\r\n    </div>\r\n\r\n\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" stripe  @selection-change=\"handleSelectionChange\">\r\n        <el-table-column v-if=\"user.role == 'ADMIN'\" type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"报账人\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"time\" label=\"报账时间\"></el-table-column>\r\n        <el-table-column prop=\"type\" label=\"报账类型\"></el-table-column>\r\n        <el-table-column prop=\"note\" label=\"备注\"></el-table-column>\r\n<!--        <el-table-column prop=\"img\" label=\"资产图片\" show-overflow-tooltip>-->\r\n<!--          <template v-slot=\"scope\">-->\r\n<!--            <div style=\"display: flex; align-items: center\">-->\r\n<!--              <el-image style=\"width: 40px; height: 40px; border-radius: 50%\" v-if=\"scope.row.img\"-->\r\n<!--                        :src=\"scope.row.img\" :preview-src-list=\"[scope.row.img]\"></el-image>-->\r\n<!--            </div>-->\r\n<!--          </template>-->\r\n<!--        </el-table-column>-->\r\n\r\n\r\n        <el-table-column label=\"操作\" width=\"180\" align=\"center\">\r\n          <template v-slot=\"scope\" v-if=\"user.role === 'ADMIN'\">\r\n            <el-button plain type=\"primary\" @click=\"handleEdit(scope.row)\" size=\"mini\">编辑</el-button>\r\n            <el-button plain type=\"danger\" size=\"mini\" @click=del(scope.row.id)>删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n\r\n    </div>\r\n\r\n    <el-dialog title=\"申请\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n        <el-form :model=\"form\" label-width=\"100px\" style=\"padding-right: 50px\" :rules=\"rules\" ref=\"formRef\">\r\n          <el-form-item label=\"类型\" prop=\"type\">\r\n            <el-input v-model=\"form.type\" placeholder=\"类型\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"时间\" prop=\"time\">\r\n            <el-input v-model=\"form.time\" placeholder=\"时间\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"备注\" prop=\"note\">\r\n            <el-input v-model=\"form.note\" placeholder=\"备注\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"头像\">\r\n            <el-upload\r\n                class=\"avatar-uploader\"\r\n                :action=\"$baseUrl + '/files/upload'\"\r\n                :headers=\"{ token: user.token }\"\r\n                list-type=\"picture\"\r\n                :on-success=\"handlePictureSuccess\"\r\n            >\r\n              <el-button type=\"primary\">上传凭证</el-button>\r\n            </el-upload>\r\n          </el-form-item>\r\n        </el-form>\r\n\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n        </div>\r\n    </el-dialog>\r\n\r\n\r\n  </div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Reimbursement\",\r\n  data(){\r\n    return{\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      fromVisible: false,\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      rules: {\r\n        name: [\r\n          {required: true, message: '请输入名称', trigger: 'blur'},\r\n        ],\r\n      },\r\n      ids: []\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  mounted() {\r\n  },\r\n  methods:{\r\n\r\n    delBatch(){\r\n      if (!this.ids.length) {\r\n        this.$message.warning('请选择数据')\r\n        return\r\n      }\r\n      this.$confirm('您确定批量删除这些数据吗？', '确认删除',{type: \"warning\"}).then(response => {\r\n        this.$request.delete(\"\")\r\n      })\r\n    },\r\n\r\n    handleAdd(){\r\n      this.form = {}  // 新增数据的时候清空数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    load(pageNum){\r\n      if(pageNum) this.pageNum = pageNum\r\n      this.$request.get(\"/reimbursement/selectPage\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n          name:this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.table = res.data?.total\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n    handlePictureSuccess(response, file, fileList) {\r\n      this.form.img = response.data\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n\r\n<!-- 报账-->"],"mappings":"AA8FA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC